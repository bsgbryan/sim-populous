.container-fluid
  .row
    .menu.col-md-3
      h1.col-xs-12 SimPopulous
      h3.col-xs-12 Residential
      .residential.tile.col-xs-3(ng-repeat="r in residential" class="{{ r }}")
      h3.col-xs-12 Commercial
      .commercial.tile.col-xs-3(ng-repeat="c in commercial" class="{{ c }}")
      h3.col-xs-12 Industrial
      .industrial.tile.col-xs-3(ng-repeat="i in industrial" class="{{ i }}")
      h3.col-xs-12 Education
      .education.tile.col-xs-3(ng-repeat="e in education" class="{{ e }}")
      h3.col-xs-12 Public
      .public.tile.col-xs-3(ng-repeat="p in public" class="{{ p }}")
    .game-board.col-md-9
      .grid
        .line(ng-repeat="col in grid")
          .tile(ng-repeat="r in col")
